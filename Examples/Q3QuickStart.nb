     TagBox[
      TemplateBox[{
        TemplateBox[{"c"}, "RowDefault"], 
        TemplateBox[{"2"}, "RowDefault"], 
        TemplateBox[{"\"\[Dagger]\""}, "RowDefault"]},
       "Subsuperscript"],
      DisplayForm],
     DisplayForm], 
    TagBox[
     SubscriptBox[
      TemplateBox[{"c"},
       "RowDefault"], 
      TemplateBox[{"2"},
       "RowDefault"]],
     DisplayForm]}],
   DisplayForm], "+", 
