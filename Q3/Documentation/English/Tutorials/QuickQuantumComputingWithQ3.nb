    BaseStyle->"InlineFormula"], "[", 
   RowBox[{
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["n", "TI"]]}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "9c8ec981-08e3-4767-a769-3786884b6574"],
