Cell["\<\
Like state vectors, Q3 provides two different ways to represent the Pauli \
operators.\
\>", "TechNoteText",
 CellID->88566011,ExpressionUUID->"88f35bbd-fc6f-4afd-91ca-6b235f3bfa9f"],

Cell[TextData[{
 "Using ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Pauli"]], "paclet:Q3/ref/Pauli", "Q3 Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   RowBox[{
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["\[Mu]", "TR"], 
     StyleBox["n", "TI"]]}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "9c8ec981-08e3-4767-a769-3786884b6574"],
 " to represent tensor product ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["S", 
     SubscriptBox["\[Mu]", "1"]], "\[CircleTimes]", 
    SuperscriptBox["S", 
     SubscriptBox["\[Mu]", "2"]], "\[CircleTimes]", "\[Ellipsis]", 
    "\[CircleTimes]", 
    SuperscriptBox["S", 
     SubscriptBox["\[Mu]", "n"]]}], TraditionalForm]],ExpressionUUID->
  "7c82538a-bcce-47d1-955b-9ddf9f702fe8"],
 ". This method works consistently with the method using ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Ket"]], "paclet:ref/Ket"},
    "RefLink",
    BaseStyle->{"InlineFormula"}], "[", 
   RowBox[{
    SubscriptBox[
     StyleBox["s", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["s", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["s", "TI"], 
     StyleBox["n", "TI"]]}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "4cb22cda-1ce3-4ed3-bf8b-5dacc2428ed8"],
 " for tensor-product basis states. This method is suitable for a small \
system of qubits."
}], "Item",
 CellID->322779506,ExpressionUUID->"9689f0a1-25b1-4f06-8367-ddfdfdc30b8f"],

Cell[TextData[{
 "Using ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"S", "[", 
    RowBox[{
     SubscriptBox[
      StyleBox["j", "TI"], 
      StyleBox["1", "TR"]], ",", 
     SubscriptBox[
      StyleBox["\[Mu]", "TR"], 
      StyleBox["1", "TR"]]}], "]"}], "**", 
   RowBox[{"S", "[", 
    RowBox[{
     SubscriptBox[
      StyleBox["j", "TI"], 
      StyleBox["2", "TR"]], ",", 
     SubscriptBox[
      StyleBox["\[Mu]", "TR"], 
      StyleBox["2", "TR"]]}], "]"}], "**", "\[CenterEllipsis]", "**", 
   RowBox[{"S", "[", 
    RowBox[{
     SubscriptBox[
      StyleBox["j", "TI"], 
      StyleBox["n", "TI"]], ",", 
     SubscriptBox[
      StyleBox["\[Mu]", "TR"], 
      StyleBox["n", "TI"]]}], "]"}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "3586b065-f91a-4a7b-93be-0ede787227fb"],
