Cell[TextData[{
 StyleBox["Figure 2",
  FontWeight->"Bold"],
 ". A reversible version of classical oracle ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "m"], "\[RightArrow]", 
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"]}]}], TraditionalForm]],
  ExpressionUUID->"50f9c1e7-be84-4e19-b32c-7a3378557c37"],
 ".  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Element]", 
    SuperscriptBox[
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}], "m"]}], TraditionalForm]],
  ExpressionUUID->"cd0d1842-e8c1-4e52-bf6b-109ed316898d"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox[
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}], "n"]}], TraditionalForm]],
  ExpressionUUID->"82a118c0-8fb2-4ea2-a1b4-e8f0a0fb5a1e"],
 " is ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],ExpressionUUID->
  "9565adbd-e66d-47bd-8c56-40005fcd7646"],
 "-bit and ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "f8fad9e6-07a5-492e-97f4-f30826f0ccc1"],
 "-bit strings, respectively, and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], "\[Element]", 
