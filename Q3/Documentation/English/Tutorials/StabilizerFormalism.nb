 CellID->2090860113,ExpressionUUID->"3b309232-888b-4549-99ab-ef02216fb3b5"],

Cell[TextData[{
 "The ",
 StyleBox[ButtonBox["full Clifford group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullCliffordGroup"],
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"e77ff9a6-20db-4969-94e3-109e6823c065"],
 " on ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "8ee2e89c-c58b-4f89-82d8-a481686e31f4"],
 " qubits is the group of unitary operators ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "f2336e7f-740e-49cd-a065-da2825c58083"],
 " that leave the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullPauliGroup"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"abc42a46-03f2-446c-9dd2-bda7859fbf32"],
 " invariant under conjugation, that is,"
}], "TechNoteText",
 CellID->1811003330,ExpressionUUID->"e7970219-d611-4cb2-95ee-bcc6ce57572c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], ":=", 
    RowBox[{"{", 
     RowBox[{"U", ":", 
      RowBox[{
       RowBox[{"U", " ", "P", " ", 
        SuperscriptBox["U", "\[Dagger]"]}], "\[Element]", 
       RowBox[{
        RowBox[{
         OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "  ", 
        "for", " ", "all", " ", "P"}], "\[Element]", 
       RowBox[{
        OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}]}]}], "}"}]}],
    TraditionalForm]],ExpressionUUID->
  "97bc5d15-e702-4668-847e-27a24fc4598e"],
 "."
}], "DisplayMath",
 CellID->1247238379,ExpressionUUID->"a67f5c2d-3894-440c-8bdf-b6c9da911f41"],

Cell[TextData[{
