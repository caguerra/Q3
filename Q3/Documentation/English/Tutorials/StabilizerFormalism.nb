 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "I"}], TraditionalForm]],ExpressionUUID->
  "f7c5d3d6-d2fc-475b-82d9-8d627bbdef9d"],
 ". The elements that square to ",
 Cell[BoxData[
  FormBox["I", TraditionalForm]],ExpressionUUID->
  "8e93cee3-4237-4c64-b075-a5ce8955c67c"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "1"}], TraditionalForm]],ExpressionUUID->
  "ea156532-4173-470e-9b4f-2bcd6b6dfaec"],
 " whereas those that square to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Minus]", "I"}], TraditionalForm]],ExpressionUUID->
  "ef934032-0edd-42e9-82a5-6bb0ee136a83"],
 " have eigenvalues ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", "\[ImaginaryI]"}], TraditionalForm]],
  ExpressionUUID->"7afa5cad-5d99-4388-9c3c-1be2bae5a2d6"],
 "."
}], "Item",
 CellID->165807436,ExpressionUUID->"19c3cf25-161b-4505-a787-bfdde5b993a9"],

