 FormBox[
  RowBox[{
   RowBox[{"X", "<->", 
    RowBox[{"(", 
     RowBox[{"1", ",", "0"}], ")"}]}], ",", " ", 
   RowBox[{"Z", "<->", 
    RowBox[{"(", 
