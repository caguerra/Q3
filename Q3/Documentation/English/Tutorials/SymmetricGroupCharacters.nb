   {
    ButtonBox["SymmetricGroupCharacters",
     BaseStyle->"Link",
     ButtonData->"paclet:Q3/ref/SymmetricGroupCharacters"], Cell["\<\
Returns the table of characters of irreducible representations of a symmetric \
group.\
\>", "TableText"]},
   {
    ButtonBox["SymmetricGroupCentralizerSize",
     BaseStyle->"Link",
     ButtonData->"paclet:Q3/ref/SymmetricGroupCentralizerSize"], Cell["\<\
Returns the number of elements in the centralizer of an element in the \
conjugacy class of the symmeric group.\
\>", "TableText"]},
   {
    ButtonBox["GroupCharacters",
     BaseStyle->"Link",
     ButtonData->"paclet:Q3/ref/GroupCharacters"], Cell["\<\
Returns the table of characters of irreducible representations of a group.\
\>", "TableText"]}
  }]], "DefinitionBox",
 CellID->1604433489],

