 RowBox[{
  RowBox[{"matJ", "=", 
   RowBox[{"SymmetricGroupCentralizerSize", "/@", 
    RowBox[{"IntegerPartitions", "[", "$n", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"matJ", "=", 
   RowBox[{"DiagonalMatrix", "[", 
    RowBox[{"1", "/", "matJ"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"matJ", "//", "MatrixForm"}]}], "Input",
