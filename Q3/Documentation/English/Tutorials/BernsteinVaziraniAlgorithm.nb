Cell[TextData[{
 "Suppose that we are given a binary function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "n"], "\[RightArrow]", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}]}], TraditionalForm]],ExpressionUUID->
  "8fbd6cde-31be-4c07-a27c-74f33ee50266"],
 ", the value of which is determined by a secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "22f93383-b777-4aa9-969c-2ed2c574ccf6"],
 " of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "4da2179b-a2d7-4484-aa08-21f1d1a25bc2"],
 " bits"
}], "TechNoteText",
 CellID->756185225,ExpressionUUID->"a5897801-c38a-40de-ba82-229dac99db09"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{"x", "\[RightTeeArrow]", 
     RowBox[{
      RowBox[{"x", "\[CenterDot]", "s"}], "   ", 
      RowBox[{"(", 
       RowBox[{"mod", " ", "2"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"6ac6eeb3-267b-4dc7-9301-70a5778fbe72"],
 "."
}], "DisplayMath",
 CellID->396853629,ExpressionUUID->"2bce7b2c-ff57-4865-b7fd-fbb4101e86fd"],

Cell[TextData[{
 "The Bernstein-Vazirani problem finds the secret bit string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "a7832f85-29c8-4e08-9d8f-da20a3857f3e"],
 " by making queries to the oracle ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "161c701f-1329-406a-94d6-0072278ff755"],
 ". Classically, one needs ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "ad43f428-b4cd-47b8-b087-63ed824477d2"],
 " queries to the function f to infer the secret string ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]],ExpressionUUID->
  "40756c52-4975-4d1e-8fad-d49e21c040db"],
 "."
}], "TechNoteText",
