Cell["\<\
Let us now examine the probabilities for the qubit to be found in the logical \
basis states.\
\>", "TechNoteMathCaption",
 CellID->353522871,ExpressionUUID->"7c69755d-1e23-4476-8219-eb0f5b0d0545"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"prob", "[", "t_", "]"}], "=", 
  RowBox[{
   RowBox[{"Abs", "[", 
    RowBox[{"ExpToTrig", "@", 
     RowBox[{"Normal", "@", 
      RowBox[{"Matrix", "@", 
       RowBox[{"vec", "[", "t", "]"}]}]}]}], "]"}], "^", "2"}]}]], "Input",
 CellLabel->"In[19]:=",
 CellID->540399814,ExpressionUUID->"3a477ce2-8a1a-46b5-8a67-4f5c97b256a1"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   SuperscriptBox[
    InterpretationBox[
     RowBox[{"\[LeftBracketingBar]", 
      RowBox[{
       RowBox[{"Cos", "[", 
        FractionBox["t", "2"], "]"}], "-", 
       FractionBox[
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"Sin", "[", 
          FractionBox["t", "2"], "]"}]}], 
        SqrtBox["2"]]}], "\[RightBracketingBar]"}],
     Abs[Cos[
       Rational[1, 2] $CellContext`t] + (Complex[0, -1] 2^Rational[-1, 2]) 
       Sin[Rational[1, 2] $CellContext`t]]], "2"], ",", 
   RowBox[{
    FractionBox["1", "2"], " ", 
    SuperscriptBox[
     InterpretationBox[
      RowBox[{"\[LeftBracketingBar]", 
       RowBox[{"Sin", "[", 
        FractionBox["t", "2"], "]"}], "\[RightBracketingBar]"}],
      Abs[
       Sin[Rational[1, 2] $CellContext`t]]], "2"]}]}], "}"}]], "Output",
 CellLabel->"Out[19]=",
 CellID->150634242,ExpressionUUID->"bb7b2120-b1c8-4dd8-9945-1e3280cc9edb"]
}, Open  ]],

Cell[CellGroupData[{

