Cell[TextData[{
 "\tInterestingly, the correspondence between the ",
 ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/PauliGroup"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"41b5b78a-2fd4-45ec-bfa9-1014123c41c0"],
