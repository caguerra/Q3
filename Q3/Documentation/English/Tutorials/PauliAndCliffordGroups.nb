 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "f64cc964-9ec8-49f3-a19a-ee9c272b87ee"],
 "."
}], "TechNoteText",
 CellID->776572750,ExpressionUUID->"af9cecae-de02-4736-a4a0-fc293cfad1e6"],

