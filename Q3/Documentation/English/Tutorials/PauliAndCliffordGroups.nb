Cell[TextData[{
 "\tIgnoring the phase factors leads to a drastically simple structure of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"936e111d-97f4-4a33-be5a-0ed399dcbbef"],
 ", compared to the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"8924ed83-565b-4503-a52e-469ddef685b6"],
 ". For example, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", " ", "Y"}], TraditionalForm]],ExpressionUUID->
  "824ca853-cd82-4316-b863-dd2f69371c42"],
 " is now equal to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", " ", "X"}], TraditionalForm]],ExpressionUUID->
  "1d93175e-9e03-4669-a814-2de31fe34a63"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", "X\[ScriptCapitalZ]", ")"}], 
     RowBox[{"(", "Y\[ScriptCapitalZ]", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"X", " ", "Y"}], ")"}], "\[ScriptCapitalZ]"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Y", " ", "X"}], ")"}], "\[ScriptCapitalZ]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"ef611fcc-9328-42c3-ba83-7976d28c9a02"],
 ". This means that unlike the ",
 ButtonBox["full Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullPauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"65bf7c5a-a8b8-4bf4-8e91-b3a0ae25704b"],
 ", the ",
 ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/PauliGroup"],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"febcb462-12bb-4d8c-893b-b8ea8790f84e"],
 " is ",
 StyleBox["Abelian",
  FontSlant->"Italic"],
 "."
}], "TechNoteText",
 CellID->360346922,ExpressionUUID->"3081b24a-7e5a-4eb7-a2c1-f50ab4f210ea"],

Cell[TextData[{
 "\tMoreover, since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"X", " ", "Z"}], "=", 
    RowBox[{
     RowBox[{"Z", " ", "X"}], "=", "Y"}]}], TraditionalForm]],ExpressionUUID->
  "c2afc129-3d57-4542-a675-a27d7e24aa36"],
 " in the ",
 ButtonBox["Pauli group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/PauliGroup"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"deb1bd8e-e2e2-4dd6-b29e-f4d33857c8b2"],
 ", the latter is isomorphic to ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
    RowBox[{"2", "n"}]], TraditionalForm]],ExpressionUUID->
  "84673fb3-7007-499d-8170-01493e935d3f"],
 " with group multiplication given by addition modulo 2."
}], "TechNoteText",
 CellID->446833514,ExpressionUUID->"5f6b75eb-1080-400c-94fc-1a5aeb8c95bf"]
}, Open  ]],

