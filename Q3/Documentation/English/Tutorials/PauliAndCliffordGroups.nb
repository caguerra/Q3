Cell[TextData[{
 "The ",
 StyleBox[ButtonBox["full Clifford group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/FullCliffordGroup"],
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
