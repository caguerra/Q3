      RowBox[{
       SubscriptBox["\[Sum]", "a"], 
       RowBox[{"a", 
        TemplateBox[{"a"},
         "Bra"], "\[Rho]", 
        TemplateBox[{"a"},
         "Ket"]}]}], "=", 
      RowBox[{"Tr", " ", "A", " ", "\[Rho]"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"b96f93eb-2ef4-4b61-95e9-66e08b57f36e"],
 " ."
}], "TechNoteText",
