Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "int"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "a"], 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "\[ImaginaryI]"}], " ", "g", " ", "a", " ", "P"}]], 
       "\[CircleTimes]", 
       TemplateBox[{"a"},
        "Ket"]}], 
      TemplateBox[{"a"},
       "Bra"]}]}]}], TraditionalForm]],ExpressionUUID->
  "66da4dae-4d38-43af-b867-bdc32dd869a4"]
}], "TechNoteText",
