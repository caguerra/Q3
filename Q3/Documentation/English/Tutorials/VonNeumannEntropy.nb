Cell["\<\
Consider a quantum state described by the following density operator.\
\>", "TechNoteMathCaption",
 CellID->831059677,ExpressionUUID->"78b01f66-68b5-448f-89d7-b332821180d7"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"rho", "=", 
   RowBox[{
    RowBox[{"1", "/", "2"}], "+", 
    RowBox[{
     RowBox[{"S", "[", "3", "]"}], "/", "4"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rho", "//", "Matrix"}], "//", "MatrixForm"}]}], "Input",
 CellLabel->"In[44]:=",
 CellID->386351251,ExpressionUUID->"cd245925-f81c-4a94-a43d-5e6d561d65b5"],

Cell[BoxData[
 TagBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      FractionBox["3", "4"], "0"},
     {"0", 
      FractionBox["1", "4"]}
    },
    GridBoxAlignment->{"Columns" -> {{Center}}, "Rows" -> {{Baseline}}},
    GridBoxSpacings->{"Columns" -> {
        Offset[0.27999999999999997`], {
         Offset[0.7]}, 
        Offset[0.27999999999999997`]}, "Rows" -> {
        Offset[0.2], {
         Offset[0.4]}, 
        Offset[0.2]}}], "\[NoBreak]", ")"}],
  Function[BoxForm`e$, 
   MatrixForm[
   SparseArray[
    Automatic, {2, 2}, 0, {
     1, {{0, 1, 2}, {{1}, {2}}}, {
      Rational[3, 4], Rational[1, 4]}}]]]]], "Output",
 CellLabel->"Out[45]//MatrixForm=",
 CellID->949498766,ExpressionUUID->"eff46dcf-2eed-4d5a-aa43-1440b0bd0aa2"]
}, Open  ]],

Cell["Here is the von Neumann entropy of the above state.", \
"TechNoteMathCaption",
 CellID->1562410676,ExpressionUUID->"a122a14a-b009-42d7-9f63-4db9d9a3e543"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"VonNeumannEntropy", "[", "rho", "]"}]], "Input",
 CellLabel->"In[46]:=",
 CellID->6196016,ExpressionUUID->"7199e030-f4a8-48dd-8e3e-a5dd03d443a0"],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], "+", 
  FractionBox[
   RowBox[{"3", " ", 
    RowBox[{"Log", "[", 
     FractionBox["4", "3"], "]"}]}], 
   RowBox[{"4", " ", 
