  TraditionalForm]],ExpressionUUID->"e9efb766-2c03-4abc-979c-3a99fd2077ec"]], \
"DisplayMath",
 CellID->1332388423,ExpressionUUID->"e7eebf3a-7622-4354-af7f-9979953faec8"],

