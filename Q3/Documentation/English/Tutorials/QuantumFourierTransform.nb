            RowBox[{"j", "-", "k", "-", "1"}], "]"}]}], "]"}], ",", 
         RowBox[{"\"\<Label\>\"", "->", 
          RowBox[{"Subscript", "[", 
           RowBox[{"\"\<T\>\"", ",", 
            RowBox[{"k", "-", "j"}]}], "]"}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"j", ",", 
         RowBox[{"k", "-", "1"}]}], "}"}]}], "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"S", "[", 
     RowBox[{"k", ",", "6"}], "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
