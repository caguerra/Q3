Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["U", "QFT"], 
    TemplateBox[{
      SubscriptBox["X", "y"]},
     "Ket"]}], ":=", 
   RowBox[{
    FractionBox["1", 
     SuperscriptBox["2", 
      RowBox[{"n", "/", "2"}]]], 
    UnderoverscriptBox["\[CapitalSigma]", 
