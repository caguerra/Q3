 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Mu]", "=", "0"}], ",", "1", ",", "2", ",", "3"}], 
   TraditionalForm]],ExpressionUUID->"6bd18b82-3389-462e-975a-9b7edcb1ee62"],
