      RowBox[{"Ket", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"S", "[", "1", "]"}], "->", "x"}], ",", 
        RowBox[{
         RowBox[{"S", "[", "6", "]"}], "->", "y"}]}], "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"qc", "=", 
