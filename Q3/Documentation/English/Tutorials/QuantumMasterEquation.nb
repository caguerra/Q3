Cell[TextData[{
 "where the additional factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", "\[ImaginaryI]"}], TraditionalForm]],ExpressionUUID->
  "b491c25c-5b59-4462-ace9-bc31a071b88f"],
 " is completely arbitrary at this stage but is introduced here for later \
convenience. The rest should vanish ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], "\[RightArrow]", 
    "0"}], TraditionalForm]],ExpressionUUID->
  "278c5fac-d1e7-4f01-ac34-73e6d78c0077"],
 " for any ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "f63c195e-23dc-4284-a603-27cf91d30b37"],
 ". Since we physically expect that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "'"}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{"\[Rho]", "(", "t", ")"}], "+", 
     RowBox[{"\[ScriptCapitalO]", "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"4edf07a9-33e8-4e31-83d5-052374e0f4f3"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "\[Mu]"], "(", 
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], TraditionalForm]],
  ExpressionUUID->"16e4daf6-6187-4df0-a8ed-1eeca58c7e13"],
 " must vanish like ",
 Cell[BoxData[
  FormBox[
   SqrtBox[
    RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]], TraditionalForm]],
  ExpressionUUID->"e03330c1-750f-41b0-9d6c-2283fd0d69ea"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], TraditionalForm]],
  ExpressionUUID->"bd20d0c5-da22-4299-a58d-2cdf33dd623b"],
 " so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["F", "\[Mu]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], 
     RowBox[{"\[Rho]", "(", "t", ")"}], 
     RowBox[{
      SubsuperscriptBox["F", "\[Mu]", "\[Dagger]"], "(", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], "\[TildeTilde]", 
    RowBox[{"\[ScriptCapitalO]", "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"ae8254df-6ea3-4d04-a5e1-96b5f7dc9fa2"],
 ". We put"
}], "TechNoteText",
 CellID->719905097,ExpressionUUID->"00cda737-79a6-40cd-8723-04d3a01b20b3"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "\[Mu]"], "(", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}], ")"}], "\[TildeTilde]", 
    RowBox[{
     SqrtBox[
      RowBox[{" ", 
