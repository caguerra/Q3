 CounterIncrements->"DisplayFormulaNumbered",
 CellID->188107167,ExpressionUUID->"38d2db59-e5bd-44a0-aa99-093fd9da758d"],

Cell[TextData[{
 "which resembles the unitary dynamics with Hamiltonian ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "131b26c3-4fe9-4dd3-9cb3-25638fccef72"],
 " replaced with the effective non-Hermitian Hamiltonian "
}], "TechNoteText",
 CellID->421227385,ExpressionUUID->"a119c20d-b560-4542-b489-52693002bcce"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["H", "non"], ":=", 
    RowBox[{"H", "-", 
     RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "G"}]}]}], 
   TraditionalForm]],ExpressionUUID->"e9bf7b93-b788-4f0b-a080-c94050c899f4"],
 " ."
}], "TechNoteText",
