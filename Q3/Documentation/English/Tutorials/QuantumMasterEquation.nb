        {
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
          "\[CapitalDelta]"}], "0", 
         RowBox[{
          RowBox[{"-", 
           SubscriptBox["\[CapitalGamma]", "-"]}], "-", 
          SubscriptBox["\[CapitalGamma]", "+"], "+", 
          RowBox[{
          "2", "\[ImaginaryI]", "\[VeryThinSpace]", "\[CapitalOmega]"}]}], 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "\[CapitalDelta]"}]},
        {
         RowBox[{"2", 
          SubscriptBox["\[CapitalGamma]", "-"]}], 
         RowBox[{
          RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", 
          "\[CapitalDelta]"}], 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "\[CapitalDelta]"}], 
         RowBox[{
          RowBox[{"-", "2"}], 
          SubscriptBox["\[CapitalGamma]", "+"]}]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["\[Rho]", "11"]},
        {
         SubscriptBox["\[Rho]", "12"]},
        {
         SubscriptBox["\[Rho]", "21"]},
        {
         SubscriptBox["\[Rho]", "22"]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "988a190d-c24c-430f-ae31-cd5891e55103"]
}], "TechNoteText",
 CellID->866849901,ExpressionUUID->"9cc4965d-ec0d-445b-ab00-8645655cc974"],

