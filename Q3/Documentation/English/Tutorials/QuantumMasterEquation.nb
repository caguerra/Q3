Cell[TextData[{
 "Since ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["L", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "68abe2b9-06d8-45f9-a869-25cb41d8a301"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "6106d181-bead-43ad-b151-d81193d72e4a"],
 ") directly proportional to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["F", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "ef24a09a-f01b-4129-9677-9a910cb3d703"],
 ", they are all traceless and mutually orthogonal; i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "[", 
     RowBox[{
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"], 
      SubscriptBox["L", "\[Mu]"]}], "]"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"14d42f33-5a8a-4a3c-bb50-2e2aecf3f4a9"],
 " for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "\[Nu]"}], TraditionalForm]],
  ExpressionUUID->"cfcea4b5-ccae-4e59-8761-a71ed20b46bc"],
 ". Putting these approximations back into the equation for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "(", 
    RowBox[{"t", "'"}], ")"}], TraditionalForm]],ExpressionUUID->
  "79514326-e7a4-4164-bf45-85453a26df12"],
 " at the beginning of this section leads to"
}], "TechNoteText",
 CellID->284265725,ExpressionUUID->"cb21565f-316e-416a-8daf-33b8fc8c98b8"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]}], ")"}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"I", "-", 
        RowBox[{
        "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
         "t", "\[VeryThinSpace]", 
         SubscriptBox["L", "0"]}]}], ")"}], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", 
         RowBox[{
         "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
          "t", "\[VeryThinSpace]", 
          SubscriptBox["L", "0"]}]}], ")"}], "\[Dagger]"]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}], "\[TildeTilde]", 
    RowBox[{
     RowBox[{"\[Rho]", "(", "t", ")"}], "-", 
     RowBox[{
     "\[ImaginaryI]", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", 
      RowBox[{"t", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["L", "0"], 
         RowBox[{"\[Rho]", "(", "t", ")"}]}], "-", 
        RowBox[{
         RowBox[{"\[Rho]", "(", "t", ")"}], 
         SubsuperscriptBox["L", "0", "\[Dagger]"]}]}], "]"}]}], "+", 
     RowBox[{"\[Delta]", "\[VeryThinSpace]", "t", "\[VeryThinSpace]", 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"\[Mu]", "=", "1"}], 
       RowBox[{
        SuperscriptBox["d", "2"], "-", "1"}]], 
      SubscriptBox["L", "\[Mu]"], 
      RowBox[{"\[Rho]", "(", "t", ")"}], 
      SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"]}]}]}], TraditionalForm]],
  ExpressionUUID->"8d462376-0d2a-4c3d-8720-d74b3539d25c"],
 " ."
}], "TechNoteText",
