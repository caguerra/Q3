Cell["is simply given by ", "TechNoteText",
 CellID->1457519728,ExpressionUUID->"720855fd-c649-4ae8-9446-c459fb738b58"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", "t", ")"}], "\[TildeTilde]", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], "\[VeryThinSpace]", "t", 
       "\[VeryThinSpace]", 
       SubscriptBox["H", "non"]}]], 
     RowBox[{"\[Rho]", "(", "0", ")"}], 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"+", "\[ImaginaryI]"}], "\[VeryThinSpace]", "t", 
       "\[VeryThinSpace]", 
       SubsuperscriptBox["H", "non", "\[Dagger]"]}]]}]}], TraditionalForm]],
  ExpressionUUID->"32605647-7ed7-4e9b-9338-50109d8c37bc"],
 " ,"
}], "TechNoteText",
