  FormBox[
   SubscriptBox["L", "\[Mu]"], TraditionalForm]],ExpressionUUID->
  "ca59c7ea-f38d-47ac-9512-9aec4a252600"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[NotEqual]", "0"}], TraditionalForm]],ExpressionUUID->
  "719de4e3-e9ab-44f9-8601-6ab72b766360"],
 " through the relation"
}], "TechNoteText",
 CellID->1818842823,ExpressionUUID->"59341971-9457-4f0c-a577-07c755c2af0a"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", ":=", 
    RowBox[{
     FractionBox["1", "2"], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"\[Mu]", "=", "1"}], 
      RowBox[{
       SuperscriptBox["d", "2"], "-", "1"}]], 
     SubsuperscriptBox["L", "\[Mu]", "\[Dagger]"], 
     SubscriptBox["L", "\[Mu]"]}]}], TraditionalForm]],ExpressionUUID->
