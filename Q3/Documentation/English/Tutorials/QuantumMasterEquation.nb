 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", "\[VeryThinSpace]", 
     SubscriptBox["F", "\[Mu]"]}], ":=", 
    RowBox[{
     SqrtBox[
      RowBox[{"\[Delta]", "\[VeryThinSpace]", "t"}]], 
     SubscriptBox["L", "\[Mu]"]}]}], TraditionalForm]],ExpressionUUID->
  "ba803703-999a-4d48-ac7c-24417d6bcfc6"],
 " with aforementioned probability at each time step ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["t", "k"], ":=", 
    RowBox[{"k", "\[VeryThinSpace]", "\[Delta]", "\[VeryThinSpace]", "t"}]}], 
   TraditionalForm]],ExpressionUUID->"3f5a39fa-e2f4-4c87-b30a-314875a9ddfa"],
