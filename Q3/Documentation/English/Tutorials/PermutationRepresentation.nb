 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      TemplateBox[{
        SubscriptBox["s", "1"]},
       "Ket"], ",", 
      TemplateBox[{
        SubscriptBox["s", "2"]},
       "Ket"], ",", "\[Ellipsis]", ",", 
      TemplateBox[{
        SubscriptBox["s", "n"]},
       "Ket"]}], "}"}], ","}], TraditionalForm]],ExpressionUUID->
  "84e1b2b9-09b4-4084-9d92-eb463a05135f"],
 " and the action of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "\[Element]", "\[ScriptCapitalG]"}], TraditionalForm]],
  ExpressionUUID->"df2c7455-6c20-4e5d-ac83-16a8a6ac65a5"],
