 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"(", 
     SubscriptBox["w", "\[Lambda]"], ")"}], 
    RowBox[{"d", "-", "1"}]], TraditionalForm]],ExpressionUUID->
  "9f2e82b0-d362-49ff-a7c4-63cee8c17ac7"],
