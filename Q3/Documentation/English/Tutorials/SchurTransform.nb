 Cell[BoxData[
  SuperscriptBox["\[ScriptCapitalW]", "\[Lambda]"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "c1adcde1-eaf3-4534-aa1c-1e85f780a360"],
 ". The dimension of ",
 Cell[BoxData[
  SuperscriptBox["\[ScriptCapitalW]", "\[Lambda]"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "d2f4db4f-ebd9-4bdc-a8c7-23622b976302"],
 " equals the number of ",
 StyleBox["Weyl tableaux",
  FontSlant->"Italic"],
