     SubscriptBox["\[Sum]", "x"], 
     RowBox[{
      TemplateBox[{
        RowBox[{"\[Lambda]", ",", 
          SubscriptBox[
           RowBox[{"(", 
             SubscriptBox["x", "\[Lambda]"], ")"}], 
           RowBox[{"n", "-", "1"}]]}]},
       "Ket"], 
      RowBox[{
       SubsuperscriptBox["\[CapitalPi]", 
        RowBox[{"x", " ", "y"}], "\[Lambda]"], "(", "S", ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"4da17b03-528b-4af8-af2b-0dd144c5319c"],
 ","
}], "DisplayMath",
 CellID->1800144916,ExpressionUUID->"8ebe5016-fa40-4c27-9de8-28e2679e176b"],

