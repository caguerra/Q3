Cell["\tNow the Clebsch-Gordan transform gives decomposition", "Text",
 CellID->864206507,ExpressionUUID->"b265f9e6-19fd-4f42-80a8-368155ecb8b0"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      TemplateBox[{"i"},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"j"},
       "Ket"]}], "\[Congruent]", 
     RowBox[{
      TemplateBox[{
        RowBox[{
          RowBox[{"(", "1", ")"}], ",", 
          SubscriptBox[
           RowBox[{"(", 
             GridBox[{{"\[Mu]"}, {
                RowBox[{"(", 
                  SubscriptBox["v", "\[Mu]"], ")"}]}}], ")"}], "d"]}]},
       "Ket"], "\[CircleTimes]", 
      TemplateBox[{"j"},
       "Ket"]}]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "\[Lambda]"], 
     RowBox[{
      SubscriptBox["\[Sum]", 
       RowBox[{"(", 
        SubscriptBox["w", "\[Lambda]"], ")"}]], 
      RowBox[{
       TemplateBox[{
         RowBox[{
           SubscriptBox[
            RowBox[{"(", 
              GridBox[{{"\[Lambda]"}, {"1"}}], ")"}], "2"], ",", 
           SubscriptBox[
            RowBox[{"(", 
              GridBox[{{"\[Lambda]"}, {
                 RowBox[{"(", 
                   SubscriptBox["w", "\[Lambda]"], ")"}]}}], ")"}], "d"]}]},
        "Ket"], 
       TemplateBox[{
         SubscriptBox[
          RowBox[{"(", 
            GridBox[{{"\[Lambda]"}, {
               RowBox[{"(", 
                 SubscriptBox["w", "\[Lambda]"], ")"}]}}], ")"}], "d"], 
         RowBox[{
           SubscriptBox[
            RowBox[{"(", 
              GridBox[{{"\[Mu]"}, {
                 RowBox[{"(", 
                   SubscriptBox["v", "\[Mu]"], ")"}]}}], ")"}], "d"], ",", 
           "j"}]},
        "BraKet"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "7e8d6dc6-8c44-4a0e-a656-cb86cdf9cf1d"],
 "."
}], "DisplayMath",
 CellID->670361110,ExpressionUUID->"30307b63-7282-4352-bd04-a805831f17ff"],

