 FormBox[
  RowBox[{"(", GridBox[{
     {
      SubscriptBox["m", "n1"], 
      SubscriptBox["m", "n2"], "\[Ellipsis]", 
      SubscriptBox["m", 
       StyleBox["nn",
        FontSlant->"Italic"]]},
     {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[AscendingEllipsis]", 
      " "},
     {
      SubscriptBox["m", "21"], 
      SubscriptBox["m", "22"], " ", " "},
     {
      SubscriptBox["m", "11"], " ", " ", " "}
