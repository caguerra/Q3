 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{"i"},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"j"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     TemplateBox[{
       SubscriptBox[
        RowBox[{"(", 
          GridBox[{{"\[Mu]"}, {
             RowBox[{"(", 
               SubscriptBox["v", "\[Mu]"], ")"}]}}], ")"}], "d"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{"j"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "a1dc8721-153a-44b6-a4fa-4dc617953dc4"],
 ","
}], "DisplayMath",
