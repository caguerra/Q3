  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["y", "n"], ",", 
     SubscriptBox["w", "d"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "7cac7e80-d01a-4878-8f83-bf099bcc8c70"],
