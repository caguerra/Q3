Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["J",
     FontWeight->"Bold"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["J", "x"], ",", 
