Cell[TextData[{
 "The full Pauli group on ",
 StyleBox["n",
  FontSlant->"Italic"],
 " qubits, denoted by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"5383a6f2-2fbb-432b-b06e-51cc384f6996"],
 ", is the group of tensor products of single-qubit Pauli operators. The \
center of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"c2b4ddf6-83ff-4bf6-a934-2fc4ce77e7e8"],
 " is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{"I", ",", " ", 
      RowBox[{"-", "I"}], ",", " ", 
      RowBox[{"\[ImaginaryI]", " ", "I"}], ",", " ", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "I"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"f5accb96-6683-4b2f-be10-9895cf1e4e81"],
 ". The Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"6fba3db8-2b6a-4131-b502-028dafb1a0a3"],
 " is the quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "/", 
     "\[ScriptCapitalZ]"}]}], TraditionalForm]],ExpressionUUID->
  "9eb9b643-0e69-413e-8bbe-4dc8f0acf011"],
 ". The Pauli group is isomorphic to the Abelian group ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
    RowBox[{"2", "n"}]], TraditionalForm]],ExpressionUUID->
  "ae71e0d0-6077-49ab-8e16-9188fec7b37d"],
 ", which can also be regarded as a vector space over ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[DoubleStruckCapitalZ]", "2"], TraditionalForm]],
  ExpressionUUID->"9610f4e3-5205-4653-be30-15ecc13eb974"],
 "."
}], "Notes",
 CellID->447549871,ExpressionUUID->"07f36c77-c0be-46d4-aad1-f2859fa7758e"]
