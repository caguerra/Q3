Cell[TextData[{
 "The full Pauli group on ",
 StyleBox["n",
  FontSlant->"Italic"],
 " qubits, denoted by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"7bddf84b-fb26-4c26-a3f5-ab540ef42db4"],
 ", is the group of tensor products of single-qubit Pauli operators. The \
center of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"59394750-a674-4176-b168-ce88584c0994"],
 " is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{"I", ",", " ", 
      RowBox[{"-", "I"}], ",", " ", 
      RowBox[{"\[ImaginaryI]", " ", "I"}], ",", " ", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]"}], " ", "I"}]}], "}"}]}], 
   TraditionalForm]],ExpressionUUID->"82c772c6-1273-45f1-b984-b616a3fab17f"],
 ". The Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"38489a39-54ee-41bc-a0bf-fcc095d38445"],
 " is the quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "/", 
     RowBox[{"\[ScriptCapitalZ]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"607712d1-98fd-4c58-bfb8-b81b85ba662e"],
 ". The Pauli group is isomorphic to the Abelian group ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[DoubleStruckCapitalZ]", "2", 
    RowBox[{"2", "n"}]], TraditionalForm]],ExpressionUUID->
  "57221155-e2e2-436d-ae51-1950d6b31ee8"],
 ", which can also be regarded as a vector space over ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[DoubleStruckCapitalZ]", "2"], TraditionalForm]],
  ExpressionUUID->"58125c51-ce2c-4ca7-a638-91291271b2b6"],
 "."
}], "Notes",
