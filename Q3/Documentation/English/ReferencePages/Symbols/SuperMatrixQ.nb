Cell[TextData[{
 "Under a supermap, density matrices transform as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Rho]", 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}], LimitsPositioning -> True], 
         SubscriptBox[
          StyleBox["M", "TI"], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"], ";", 
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}]], 
         SubscriptBox["\[Rho]", 
          RowBox[{
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}]], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}], LimitsPositioning -> True], 
         SubscriptBox[
          StyleBox["C", "TI"], 
          RowBox[{
            StyleBox["i", "TI"], ",", 
            StyleBox["k", "TI"], ";", 
            StyleBox["j", "TI"], ",", 
            StyleBox["l", "TI"]}]], 
         SubscriptBox["\[Rho]", 
          RowBox[{
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "\\rho_{ij}=\\sum_{kl}M_{ij;kl}\\rho_{kl}=\\sum_{kl}C_{i,k;j,l}\\rho_{kl}\
", "state" -> "Boxes"|>,
