Cell[TextData[{
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["LindbladConvert"]], "paclet:Q3/ref/LindbladConvert", 
    "Q3 Package Symbol"},
   "PackageLink",
   BaseStyle->"InlineFormula"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "f350a83f-6612-4ce5-8437-37f93e0638e1"],
 " converts the Lindblad equation into the form\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SubscriptBox[
         OverscriptBox["\[Rho]", "."], 
         StyleBox["i", "TI"]], "\[LongEqual]", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         StyleBox["M", "TI"], 
         RowBox[{
           StyleBox["i", "TI"], 
           StyleBox["j", "TI"]}]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]], "+", 
        SubscriptBox[
         StyleBox["v", "TI"], 
         StyleBox["j", "TI"]], "  ", 
        RowBox[{"(", 
          RowBox[{
            StyleBox["i", "TI"], "\[LongEqual]", "1", ",", "\[Ellipsis]", ",", 
            SuperscriptBox[
             StyleBox["n", "TI"], "2"], "-", "1"}], ")"}]}], TraditionalForm],
     "errors" -> {}, "input" -> 
    "\\dot\\rho_i=\\sum_{j=1}^{n^2-1}M_{ij}\\rho_j+v_j \\quad \
(i=1,\\dots,n^2-1)", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "f0adf93b-22df-4388-8557-b691be578f17"],
 ",\nwhere the density operator is represented in the Lindblad basis, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"{", 
         RowBox[{
           SubscriptBox[
            OverscriptBox[
             StyleBox["B", "TI"], "^"], 
            StyleBox["j", "TI"]], ":", 
           StyleBox["j", "TI"], "\[LongEqual]", "1", ",", "2", ",", 
           "\[Ellipsis]", ",", 
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], "}"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\{\\hat{B}_j:j=1,2,\\dots,n^2-1\\}", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "109a16c3-2383-44a3-812e-1cb957ea7949"],
 ", as\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        OverscriptBox["\[Rho]", "^"], "\[LongEqual]", 
        FractionBox["1", 
         StyleBox["n", "TI"]], 
        OverscriptBox[
         StyleBox["I", "TI"], "^"], "+", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         OverscriptBox[
          StyleBox["B", "TI"], "^"], 
         StyleBox["j", "TI"]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
    "\\hat\\rho=\\frac{1}{n}\\hat{I}+\\sum_{j=1}^{n^2-1}\\hat{B}_j\\rho_j", 
    "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "816b794d-52d2-40f7-bcb6-6324899e08ab"],
 " ."
}], "Notes",
 CellID->1292051031,ExpressionUUID->"a0131815-1ec5-4b7e-8e14-8e03cfd1ff2f"],

Cell[TextData[{
 "The solution of the Lindblad equation is given by\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Rho]", 
          StyleBox["i", "TI"]], 
         RowBox[{"(", 
           StyleBox["t", "TI"], ")"}], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          StyleBox["j", "TI"], LimitsPositioning -> True], 
         SubscriptBox[
          RowBox[{"[", 
            SuperscriptBox[
             StyleBox["e", "TI"], 
             RowBox[{
               StyleBox["M", "TI"], 
               StyleBox["t", "TI"]}]], "]"}], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], 
         SubscriptBox["\[Rho]", 
          StyleBox["j", "TI"]], 
         RowBox[{"(", "0", ")"}], "+", 
         UnderscriptBox["\[Sum]", 
          StyleBox["j", "TI"], LimitsPositioning -> True], 
         SubscriptBox[
          RowBox[{"[", 
            RowBox[{
              SubsuperscriptBox["\[Integral]", "0", 
               StyleBox["t", "TI"]], 
              RowBox[{
                StyleBox["d", "TI"], 
                StyleBox["s", "TI"]}], " ", 
              SuperscriptBox[
               StyleBox["e", "TI"], 
               RowBox[{
                 StyleBox["M", "TI"], 
                 StyleBox["s", "TI"]}]]}], "]"}], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], 
         SubscriptBox[
          StyleBox["v", "TI"], 
          StyleBox["j", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\rho_i(t)=\\sum_j[e^{Mt}]_{ij}\\rho_j(0)+\\sum_j\\left[\\int_0^t{ds}\\,\
e^{Ms}\\right]_{ij}v_j", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cffe1f9f-fd1a-4546-b9fc-885fb7cb932f"],
 "."
}], "Notes",
 CellID->421100671,ExpressionUUID->"8fcbc4a1-ba86-4e0d-8b36-6c45d1828461"],

