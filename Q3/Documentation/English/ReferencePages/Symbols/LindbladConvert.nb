Cell[TextData[{
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["LindbladConvert"]], "paclet:Q3/ref/LindbladConvert", 
    "Q3 Package Symbol"},
   "PackageLink",
   BaseStyle->"InlineFormula"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "f350a83f-6612-4ce5-8437-37f93e0638e1"],
 " converts the Lindblad equation into the form\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        SubscriptBox[
         OverscriptBox["\[Rho]", "."], 
         StyleBox["i", "TI"]], "\[LongEqual]", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         StyleBox["M", "TI"], 
         RowBox[{
           StyleBox["i", "TI"], 
           StyleBox["j", "TI"]}]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]], "+", 
        SubscriptBox[
         StyleBox["v", "TI"], 
         StyleBox["j", "TI"]], "  ", 
        RowBox[{"(", 
          RowBox[{
            StyleBox["i", "TI"], "\[LongEqual]", "1", ",", "\[Ellipsis]", ",", 
            SuperscriptBox[
             StyleBox["n", "TI"], "2"], "-", "1"}], ")"}]}], TraditionalForm],
     "errors" -> {}, "input" -> 
    "\\dot\\rho_i=\\sum_{j=1}^{n^2-1}M_{ij}\\rho_j+v_j \\quad \
(i=1,\\dots,n^2-1)", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "f0adf93b-22df-4388-8557-b691be578f17"],
 ",\nwhere the density operator is represented in the Lindblad basis as\n\t",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        OverscriptBox["\[Rho]", "^"], "\[LongEqual]", 
        FractionBox["1", 
         StyleBox["n", "TI"]], "+", 
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
           StyleBox["j", "TI"], "\[LongEqual]", "1"}], 
         RowBox[{
           SuperscriptBox[
            StyleBox["n", "TI"], "2"], "-", "1"}], LimitsPositioning -> True], 
        SubscriptBox[
         OverscriptBox[
          StyleBox["B", "TI"], "^"], 
         StyleBox["j", "TI"]], 
        SubscriptBox["\[Rho]", 
         StyleBox["j", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
    "\\hat\\rho=\\frac{1}{n}+\\sum_{j=1}^{n^2-1}\\hat{B}_j\\rho_j", "state" -> 
    "Boxes"|>,
   "TeXAssistantTemplate"]], "Input",ExpressionUUID->
  "816b794d-52d2-40f7-bcb6-6324899e08ab"],
 " ."
}], "Notes",
