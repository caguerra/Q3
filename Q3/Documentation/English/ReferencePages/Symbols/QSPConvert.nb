Cell[TextData[{
 "The quantum signal processing (QSP) of degree ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "7ff1f701-aa72-4dd4-a29c-4b724246a84c"],
 " with phases ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "0"], ",", 
      SubscriptBox["\[Phi]", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "d"]}], "}"}], "=", 
    RowBox[{":", 
     OverscriptBox["\[Phi]", "\[RightVector]"]}]}], TraditionalForm]],
  ExpressionUUID->"850a7fe9-c1f3-4713-9d8a-82c9ea7333e4"],
 " is a parameterized representation of a polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "f4a6f5a4-c82b-49e9-85cb-8d85bc6b19d1"],
 " of degree ",
 Cell[BoxData[
  FormBox["d", TraditionalForm]],ExpressionUUID->
  "cc29d88a-e2df-4a5b-9a43-7c68d18b8d52"],
 " and parity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "-", 
    RowBox[{"2", 
     RowBox[{"(", 
      RowBox[{"d", " ", "mod", " ", "2"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"28035bbb-20c9-4a93-9d35-4a3de92250a6"],
 ", through the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "\[Times]", "2"}], TraditionalForm]],ExpressionUUID->
  "2236aeaa-2feb-4969-b1ff-3c6c815c351b"],
 " unitary matrix\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"]], "(", "x", ")"}], ":=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
        SubscriptBox["\[Phi]", "0"], "Z"}]], 
      RowBox[{
       UnderoverscriptBox["\[CapitalPi]", 
        RowBox[{"k", "=", "1"}], "d"], "[", 
       RowBox[{
        RowBox[{"W", "(", "x", ")"}], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          SubscriptBox["\[Phi]", "k"], "Z"}]]}], "]"}]}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"P", "(", "x", ")"}], 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
          RowBox[{"Q", "(", "x", ")"}], 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["x", "2"]}]]}]},
        {
         RowBox[{"\[VeryThinSpace]", 
          RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
           RowBox[{
            SuperscriptBox["Q", "*"], "(", "x", ")"}], 
           SqrtBox[
            RowBox[{"1", "-", 
             SuperscriptBox["x", "2"]}]]}]}], 
         RowBox[{
          SuperscriptBox["P", "*"], "(", "x", ")"}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "d339a624-2c95-4f3a-998f-e82e778d8c90"],
 " ,\nwhere ",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "0fa1439c-744d-47fe-acb9-cb7902788d3d"],
 " is the Pauli-",
 Cell[BoxData[
  FormBox["Z", TraditionalForm]],ExpressionUUID->
  "ba55efb2-0494-447d-a2e2-6f4313b03578"],
 " matrix,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"Z", ":=", 
    RowBox[{"(", GridBox[{
       {"1", "0"},
       {"0", 
        RowBox[{"-", "1"}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "161f9293-5d99-49f3-b743-86b6de2cb879"],
 " ,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "a8b6987a-889a-4c4d-ac9a-f4c50c8dac9c"],
 " is a rotation in the Bloch space around the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "1451ade3-bafa-49ba-a6f8-9ef54983262e"],
 "-axis by angle ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "2"}], 
    RowBox[{"arccos", "(", "x", ")"}]}], TraditionalForm]],ExpressionUUID->
  "57ff3de8-85af-4563-9f64-71ad21e82524"],
 ",\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"W", "(", "x", ")"}], ":=", 
    RowBox[{"(", GridBox[{
       {"x", 
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"]}]]}]},
       {
        RowBox[{"\[ImaginaryI]", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["x", "2"]}]]}], "x"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "a46b0c3b-e599-4189-a714-493d6525ecc9"],
 " ,\nand ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "2ebfc79a-e0a9-4479-a59a-b52c9b92b7b7"],
 " is a polynomial of degree ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "-", "1"}], TraditionalForm]],ExpressionUUID->
  "ba1b6cc8-5bf7-4307-964f-786da2123534"],
 " and parity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "-", 
    RowBox[{"2", 
     RowBox[{"(", 
      RowBox[{"d", "-", 
       RowBox[{"1", " ", "mod", " ", "2"}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"89ce37ca-ee27-4513-9372-eefab4016151"],
 " satisfying the unitarity constraint \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"\[LeftBracketingBar]", 
       RowBox[{"P", "(", "x", ")"}], "\[RightBracketingBar]"}], "2"], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{"Q", "(", "x", ")"}], "\[RightBracketingBar]"}], "2"], 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["x", "2"]}], ")"}]}]}], "=", "1"}], TraditionalForm]],
  ExpressionUUID->"3eadd7b0-2ce3-42ea-887f-8a9367443427"],
 "."
}], "Notes",
 CellID->958672882,ExpressionUUID->"c7d80ff0-2e00-467b-b797-c3aff93807cf"],

Cell[TextData[{
 "Quantum signal processing is ",
 StyleBox["symmetric",
  FontSlant->"Italic"],
 " if the phase vector is symmetric ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "0"], ",", 
      SubscriptBox["\[Phi]", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "d"]}], "}"}], "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "d"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "1"], ",", 
      SubscriptBox["\[Phi]", "0"]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"5d38c360-6d84-4b53-b8e3-4f6804319275"],
 " under list reversal. A symmetric quantum signal processing has its \
characteristic unitary matrix symmetric, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"]], "(", "x", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"], "T"], "(", "x", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"b39c19b3-630c-43be-99ae-6b55e5063372"],
 ", and polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "651ba76e-bd34-4f32-afb6-087b52c6900f"],
 " is real. See Theorem 2 in Dong et al. (2021)."
}], "Notes",
 CellID->1795688728,ExpressionUUID->"1679366b-fcff-4aa1-bbc5-297f3e0c032f"],

Cell[TextData[{
 "A quantum signal processing is said to be ",
 StyleBox["symmetric",
  FontSlant->"Italic"],
 " if the phase vector is symmetric; that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "0"], ",", 
      SubscriptBox["\[Phi]", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "d"]}], "}"}], "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Phi]", "d"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "1"], ",", 
      SubscriptBox["\[Phi]", "0"]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"10211c5b-5440-414c-b4b2-0747ae7015d2"],
 ", which leaves only ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "d"], ":=", 
    RowBox[{"\[LeftCeiling]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"d", "+", "1"}], ")"}], "/", "2"}], "\[RightCeiling]"}]}], 
   TraditionalForm]],ExpressionUUID->"bbe4da61-5b22-475e-aeb4-f2d74cb446b8"],
 " independent phase variables. A symmetric quantum signal processing has its \
characteristic unitary matrix symmetric, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"]], "(", "x", ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["U", 
      OverscriptBox["\[Phi]", "\[RightVector]"], "T"], "(", "x", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"064dd0ef-c524-4e80-88d3-e9b2edc6a608"],
 ", and the polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "(", "x", ")"}], TraditionalForm]],ExpressionUUID->
  "fc2fe30e-5ea2-4948-acea-8b5046bac43b"],
 " is real ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["(Dong, Meng, Whaley and Lin, 2021: Theorem 2)"]], 
    "https://doi.org/10.1103/PhysRevA.103.042419"},
   "WebLink",
   BaseStyle->{"Notes"}]],ExpressionUUID->
  "03c4aa64-c99f-492c-90a4-99b34721179f"],
 "."
}], "Notes",
 CellID->1190343635,ExpressionUUID->"57a9bcc4-30fe-4d76-8b83-61a4482c53b8"],

Cell[TextData[{
 "Given a ",
 StyleBox["reduced",
  FontSlant->"Italic"],
 " set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["\[CurlyPhi]", "0"], ",", 
     SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["\[CurlyPhi]", 
      RowBox[{
       SubscriptBox["n", "d"], "-", "1"}]]}], "}"}], TraditionalForm]],
  ExpressionUUID->"48a0b837-a6ec-4a6a-bdbb-612ea118ff63"],
 " of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "d"], TraditionalForm]],ExpressionUUID->
  "b81cbab1-68a5-492d-83ad-9f663a95c976"],
 " independent phase variables and parity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "=", 
    RowBox[{"\[PlusMinus]", "1"}]}], TraditionalForm]],ExpressionUUID->
  "53a3be2f-e02b-4bb9-86f3-ed4cbe2675c2"],
 " for a symmetric quantum signal processing, one can recover the ",
 StyleBox["full vector",
  FontSlant->"Italic"],
 " of phase variables as follows:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["\[CurlyPhi]", "0"], ",", 
        SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["\[CurlyPhi]", 
         RowBox[{
          SubscriptBox["n", "d"], "-", "1"}]]}], "}"}], ",", 
      RowBox[{"+", "1"}]}], "}"}], "\[RightTeeArrow]", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[CurlyPhi]", "0"], ",", 
      SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
      RowBox[{"2", 
       SubscriptBox["\[CurlyPhi]", 
        RowBox[{
         SubscriptBox["n", "d"], "-", "1"}]]}], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[CurlyPhi]", "1"], ",", 
      SubscriptBox["\[CurlyPhi]", "0"]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"2fe36f2f-1ce1-4142-8905-f77824218e77"],
 " (notice the factor of 2 for technical convenience),\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["\[CurlyPhi]", "0"], ",", 
        SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
        SubscriptBox["\[CurlyPhi]", 
         RowBox[{
          SubscriptBox["n", "d"], "-", "1"}]]}], "}"}], ",", 
      RowBox[{"-", "1"}]}], "}"}], "\[RightTeeArrow]", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[CurlyPhi]", "0"], ",", 
      SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[CurlyPhi]", 
       RowBox[{
        SubscriptBox["n", "d"], "-", "1"}]], ",", 
      SubscriptBox["\[CurlyPhi]", 
       RowBox[{
        SubscriptBox["n", "d"], "-", "1"}]], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[CurlyPhi]", "1"], ",", 
      SubscriptBox["\[CurlyPhi]", "0"]}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"508d398b-8fd6-4038-b35c-679f535bd1e9"],
 ".\nThe pair ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["\[CurlyPhi]", "0"], ",", 
       SubscriptBox["\[CurlyPhi]", "1"], ",", "\[Ellipsis]", ",", 
       SubscriptBox["\[CurlyPhi]", 
        RowBox[{
         SubscriptBox["n", "d"], "-", "1"}]]}], "}"}], ",", 
     RowBox[{"p", "=", 
      RowBox[{"\[PlusMinus]", "1"}]}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"bec63ba4-f3bd-40aa-b654-2207fdc7fc23"],
 " is called the ",
 StyleBox["reduced phase vector",
  FontSlant->"Italic"],
 " to be distinguished from the ",
 StyleBox["full phase vector",
  FontSlant->"Italic"],
 "."
}], "Notes",
 CellID->1535193593,ExpressionUUID->"e2613aec-f26a-47eb-8b68-49cc40fb8f3a"],

Cell[TextData[{
 "Note that in  ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["QSPPACK"]], "https://github.com/qsppack/QSPPACK"},
   "WebLink",
   BaseStyle->{"Notes"}]],ExpressionUUID->
  "36b29853-d99c-4f01-9973-e1771a23a15b"],
 " and Dong et al. (2021), the convention is to take the reduced phase vector \
of the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[Phi]", "\[RightVector]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Phi]", "n"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["\[Phi]", "1"], ",", 
      SubscriptBox["\[Phi]", "0"]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"e3075ce8-514f-4ba8-993c-46193a6193a5"],
 " to encode the symmetric phase vector ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["0", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    RowBox[{"2", 
     SubscriptBox[
      StyleBox["\[Phi]", "TR"], 
      StyleBox["n", "TI"]]}], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["0", "TR"]]}], "}"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "aaface2b-57ee-4c18-8054-55d0d989a44a"],
 " or  ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["0", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["n", "TI"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["n", "TI"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["\[Phi]", "TR"], 
     StyleBox["0", "TR"]]}], "}"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "36c79173-8ac5-4711-a216-19b4c28c0b71"],
 "."
}], "Notes",
 CellID->378695770,ExpressionUUID->"35172212-133a-4955-bda1-c9f1dbe0a07f"],

Cell[TextData[{
 "The correspondence between the reduced and full phase vector is closely \
related to that between the ",
 ButtonBox["reduced and full Chebyshev coefficients",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/ChebyshevCoefficients"],
 " list."
}], "Notes",
 CellID->227248759,ExpressionUUID->"c412065a-d964-4fb5-91dc-37944540205e"]
