  FormBox[
   RowBox[{
    SubscriptBox["x", "k"], ":=", 
    RowBox[{"cos", "(", 
     RowBox[{
      RowBox[{"\[Pi]", "(", 
       RowBox[{"k", "+", 
        RowBox[{"1", "/", "2"}]}], ")"}], "/", 
      RowBox[{"(", 
