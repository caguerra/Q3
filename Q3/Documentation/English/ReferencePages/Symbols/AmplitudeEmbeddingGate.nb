  FormBox[
   RowBox[{
    TemplateBox[{"a"},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["a", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["a", "2"]},
      "Ket"], "\[CircleTimes]", "\[Ellipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["a", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "0381031f-dc57-4c7d-9527-bd64a203df0d"],
 " and ",
