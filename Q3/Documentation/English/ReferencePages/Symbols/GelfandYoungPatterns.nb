Cell[TextData[{
 "A ",
 StyleBox["Gelfand pattern",
  FontSlant->"Italic"],
 " is called a ",
 StyleBox["Gelfand-Young pattern",
  FontSlant->"Italic"],
 " if it corresponds to a ",
 StyleBox["standard Young tableau",
  FontSlant->"Italic"],
 "."
}], "Notes",
 CellID->1166026277,ExpressionUUID->"b5ac3139-95d2-4283-a8f0-c6946e04b148"],

Cell[TextData[{
 "The resulting list must be equivalent to the one from ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["YoungTableaux"]], "paclet:Q3/ref/YoungTableaux", 
    "Q3 Package Symbol"},
   "PackageLink",
   BaseStyle->"InlineFormula"]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "1ab5d8ee-0542-4ac3-9bec-bb0850f56403"],
 ". More precisely, it must equal ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["Reverse"]], "paclet:ref/Reverse"},
    "RefLink",
    BaseStyle->{"InlineFormula"}], "[", 
   RowBox[{
    TemplateBox[{
      Cell[
       TextData["Map"]], "paclet:ref/Map"},
     "RefLink",
     BaseStyle->{"InlineFormula"}], "[", 
    RowBox[{
     RowBox[{
      TemplateBox[{
        Cell[
         TextData["ToGelfandPattern"]], "paclet:Q3/ref/ToGelfandPattern", 
        "Q3 Package Symbol"},
       "PackageLink",
       BaseStyle->"InlineFormula"], "[", 
      StyleBox["n", "TI"], "]"}], ",", " ", 
     RowBox[{
      TemplateBox[{
        Cell[
         TextData["YoungTableaux"]], "paclet:Q3/ref/YoungTableaux", 
        "Q3 Package Symbol"},
       "PackageLink",
       BaseStyle->"InlineFormula"], "[", 
      StyleBox["shape", "TI"], "]"}]}], "]"}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "1396eeb0-6a99-4b32-9e67-64b0e1a77925"],
 ", where ",
 Cell[BoxData[
  RowBox[{
   StyleBox["n", "TI"], "=", 
   RowBox[{
    TemplateBox[{
      Cell[
       TextData["Total"]], "paclet:ref/Total"},
     "RefLink",
     BaseStyle->{"InlineFormula"}], "[", 
    StyleBox["shape", "TI"], "]"}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "ad70343e-73d1-4789-9bfe-4f2e8aa4a692"],
 "."
}], "Notes",
 CellID->1793054591,ExpressionUUID->"e69d039d-fdef-441a-a2ac-7bce2e88a261"],

Cell[TextData[{
 "However, ",
