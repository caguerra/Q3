Cell[TextData[{
 "A basis is said to be symplectic with respect to an inner product \
\[LeftAngleBracket]\[CenterDot],\[CenterDot]\[RightAngleBracket] when it \
satisfies that ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     SubscriptBox[
      StyleBox["v", "TI"], 
      StyleBox["i", "TI"]], ",", 
     SubscriptBox[
      StyleBox["w", "TI"], 
      StyleBox["j", "TI"]]}], "\[RightAngleBracket]"}], "=", 
   SubscriptBox[
    StyleBox["\[Delta]", "TR"], 
    StyleBox["ij", "TI"]]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "717612d3-3602-4c0d-969a-6e274e66944a"],
 " and ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[LeftAngleBracket]", 
    RowBox[{
     SubscriptBox[
      StyleBox["v", "TI"], 
      StyleBox["i", "TI"]], ",", 
     SubscriptBox[
      StyleBox["v", "TI"], 
      StyleBox["j", "TI"]]}], "\[RightAngleBracket]"}], "=", 
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox[
       StyleBox["w", "TI"], 
       StyleBox["i", "TI"]], ",", 
      SubscriptBox[
       StyleBox["w", "TI"], 
       StyleBox["j", "TI"]]}], "\[RightAngleBracket]"}], "=", "0"}]}]], 
  "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "64bda7ab-4d46-4f8a-8e6d-e34e1fbd52aa"],
 "."
}], "Notes",
