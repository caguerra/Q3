Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]], 
     SuperscriptBox[
      TemplateBox[{"0"},
       "Ket"], 
      RowBox[{"\[CircleTimes]", "n"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", "\[CenterEllipsis]", 
      "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"x", "=", "0"}], 
        RowBox[{
         SuperscriptBox["2", "n"], "-", "1"}]], 
       TemplateBox[{"x"},
        "Ket"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "bd24db03-2354-46fc-a9d5-b0cb64c8667b"],
