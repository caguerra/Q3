Cell[TextData[{
 "When applied to each qubit in state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"0"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "bdc94ce7-898f-42cd-87b3-0026c941d8b0"],
 ", the Hadamard gate generates a linear superposition of all states in the \
computational basis,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]], 
     SuperscriptBox[
      TemplateBox[{"0"},
       "Ket"], 
      RowBox[{"\[CircleTimes]", "n"}]]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}], "\[CircleTimes]", "\[CenterEllipsis]", 
      "\[CircleTimes]", 
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         TemplateBox[{"0"},
          "Ket"], "+", 
         TemplateBox[{"1"},
          "Ket"]}], 
        SqrtBox["2"]], ")"}]}], "=", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["2", 
        RowBox[{"n", "/", "2"}]]], 
      UnderoverscriptBox["\[CapitalSigma]", 
       RowBox[{"x", "=", "0"}], 
       RowBox[{
        SuperscriptBox["2", "n"], "-", "1"}]], 
      TemplateBox[{"x"},
       "Ket"]}]}]}], TraditionalForm]],ExpressionUUID->
  "83e59e16-d104-451c-8fb5-03e2b26a8eb2"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[{"x"},
     "Ket"], ":=", 
    RowBox[{
     TemplateBox[{
       SubscriptBox["x", "1"]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "2"]},
      "Ket"], "\[CircleTimes]", "\[CenterEllipsis]", "\[CircleTimes]", 
     TemplateBox[{
       SubscriptBox["x", "n"]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "4a217794-f659-4595-9eae-4fe443da388a"],
 " for an integer ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "794df6cf-f81e-4fac-b2bf-998f0fc340a2"],
 " represented by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["x", "1"], 
       SubscriptBox["x", "2"], 
       SubscriptBox["\[Ellipsis]x", "n"]}], ")"}], "2"]}], TraditionalForm]],
  ExpressionUUID->"8d7502c7-62b8-4db4-8e3d-c00ff6df5368"],
 " in the binary digits."
}], "Notes",
 CellID->1964087756,ExpressionUUID->"db5de7d5-16ec-4587-bfb5-e7e63ff20368"],

Cell[TextData[{
 "More generally, for an arbitrary state ",
 Cell[BoxData[
  FormBox[
   TemplateBox[{"y"},
    "Ket"], TraditionalForm]],ExpressionUUID->
  "48ee585a-cecb-4208-aa55-23983d4797c2"],
 " in the computational basis,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["H", 
      RowBox[{"\[CircleTimes]", "n"}]], 
     SuperscriptBox[
      TemplateBox[{"y"},
       "Ket"], 
      RowBox[{"\[CircleTimes]", "n"}]]}], "=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["2", 
       RowBox[{"n", "/", "2"}]]], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"x", "=", "0"}], 
      RowBox[{
       SuperscriptBox["2", "n"], "-", "1"}]], 
     SuperscriptBox[
      RowBox[{
       TemplateBox[{"x"},
        "Ket"], "(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"x", "\[CenterDot]", "y"}]]}]}], TraditionalForm]],
  ExpressionUUID->"58f5b6a6-eb00-49ad-9981-667d4c3613fe"],
 " ,\[LineSeparator]where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[CenterDot]", "y"}], TraditionalForm]],ExpressionUUID->
  "1dc25530-ee77-458f-a969-f9714da9a890"],
 " denotes the bitwise inner product,\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "\[CenterDot]", "y"}], ":=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], 
      SubscriptBox["y", "1"]}], "+", 
     RowBox[{
      SubscriptBox["x", "2"], 
      SubscriptBox["y", "2"]}], "+", "\[CenterEllipsis]", "+", 
     RowBox[{
      SubscriptBox["x", "n"], 
      SubscriptBox["y", "n"], "  ", 
      RowBox[{"(", 
       RowBox[{"mod", " ", "2"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"249a583e-d8de-4e28-ac58-ad708f7a7ec8"],
 " ."
}], "Notes",
