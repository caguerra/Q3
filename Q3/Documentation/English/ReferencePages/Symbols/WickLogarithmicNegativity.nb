Cell[TextData[{
 "The anomalous Green\[CloseCurlyQuote]s function of fermion modes ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],ExpressionUUID->
  "20fdbfd2-6745-4f0e-9898-f83e1d13ca61"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], ":=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      SubscriptBox["c", "i"], 
      SubscriptBox["c", "j"]}], "\[RightAngleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"1c27993b-6956-4dcd-b0e5-371a4d95f8e9"],
 ". The matrix ",
 Cell[BoxData[
  FormBox["F", TraditionalForm]],ExpressionUUID->
  "ddb60da9-cbf6-4cdd-ae07-99e394a8e44e"],
 " is skew-symmetric, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["F", "T"], "=", 
    RowBox[{"-", "F"}]}], TraditionalForm]],ExpressionUUID->
  "f3ecdc5d-d787-4903-8b29-e8d3dae897a7"],
 "."
}], "Notes",
 CellID->1383491325,ExpressionUUID->"5113ed91-46b9-47ab-b6cd-dbca48ed3466"],

Cell[TextData[{
 "In the Nambu notation, the full Green\[CloseCurlyQuote]s function is \
defined to be ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["G", "_"], ":=", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       UnderscriptBox["c", "_"], 
       SuperscriptBox[
        UnderscriptBox["c", "_"], "\[Dagger]"]}], "\[RightAngleBracket]"}], 
     "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"G", "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], 
          RowBox[{"I", "-", 
           SuperscriptBox["G", "T"]}]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"I", "-", 
           SuperscriptBox["C", "T"]}], "F"},
         {
          SuperscriptBox["F", "\[Dagger]"], "C"}
        }], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "83b54cf2-dea3-4390-b10c-3f040afd43a8"],
 ", where  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     UnderscriptBox["c", "_"], "\[Dagger]"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SubsuperscriptBox["c", "1", "\[Dagger]"], ",", "\[Ellipsis]", ",", 
      SubsuperscriptBox["c", "n", "\[Dagger]"], ",", 
      SubscriptBox["c", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["c", "n"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "1ed7c6ed-e01a-49bb-92fe-163123bb07d8"],
 "."
}], "Notes",
 CellID->795234925,ExpressionUUID->"08f4c787-c98b-4fd2-bc35-cfa0e8439812"],

