  RowBox[{
   SubscriptBox[
    StyleBox["H",
     FontSlant->"Italic"], "non"], ":=", 
   RowBox[{
    StyleBox["H",
     FontSlant->"Italic"], "-", 
    StyleBox[
     RowBox[{"\[ImaginaryI]", 
