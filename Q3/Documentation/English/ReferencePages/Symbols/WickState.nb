Cell[TextData[{
 "For a unitary matrix ",
 Cell[BoxData[
  FormBox["U", TraditionalForm]],ExpressionUUID->
  "e56191de-e1b4-4516-b9d9-71205258090e"],
 " and Hermitian matrix ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],ExpressionUUID->
  "60ba22e0-296b-4e52-bcbb-bcd0aba8571f"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", "=", 
    RowBox[{"exp", "(", 
     RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "H"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"994b05f6-25d4-4ec8-b79b-4570eef52b46"],
 " of dimension  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"L", "\[Times]", "L"}], TraditionalForm]],ExpressionUUID->
  "d93bd1c0-61fa-46c7-8e66-5934edb2e113"],
 ", ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["WickState"]], "paclet:Q3/ref/WickState", "Q3 Package Symbol"},
    
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   RowBox[{
    StyleBox["U", "TI"], ",", 
    RowBox[{"<|", 
     RowBox[{
      RowBox[{
       SubscriptBox[
        StyleBox["c", "TI"], 
        StyleBox["1", "TR"]], "\[Rule]", 
       SubscriptBox[
        StyleBox["v", "TI"], 
        StyleBox["1", "TR"]]}], ",", 
      StyleBox["\[Ellipsis]", "TR"], ",", 
      RowBox[{
       SubscriptBox[
        StyleBox["c", "TI"], 
        StyleBox["L", "TI"]], "\[Rule]", 
       SubscriptBox[
        StyleBox["v", "TI"], 
        StyleBox["L", "TI"]]}]}], "|>"}]}], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "0ec34ffc-07b5-41d5-b638-de1cc0f7cf79"],
 " represents the many-body state\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       SubsuperscriptBox["d", "1", "\[Dagger]"], ")"}], 
      SubscriptBox["v", "1"]], 
     SuperscriptBox[
      RowBox[{"(", 
       SubsuperscriptBox["d", "2", "\[Dagger]"], ")"}], 
      SubscriptBox["v", "2"]], "\[Ellipsis]", 
     TemplateBox[{"\[SpaceIndicator]"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", 
       UnderoverscriptBox["\[CapitalSigma]", 
        RowBox[{"i", ",", 
         RowBox[{"j", "=", "1"}]}], "L"], 
       SubsuperscriptBox["c", "i", "\[Dagger]"], 
       SubscriptBox["H", 
        RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
       SubscriptBox["c", "j"]}], ")"}], 
     SuperscriptBox[
      RowBox[{"(", 
       SubsuperscriptBox["c", "1", "\[Dagger]"], ")"}], 
      SubscriptBox["v", "1"]], 
     SuperscriptBox[
      RowBox[{"(", 
       SubsuperscriptBox["c", "2", "\[Dagger]"], ")"}], 
      SubscriptBox["v", "2"]], "\[Ellipsis]", 
     TemplateBox[{"\[SpaceIndicator]"},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "803f525b-dfa7-4fa0-86b7-0896f4dc6193"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["d", "j", "\[Dagger]"], "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"i", "=", "1"}], "L"], 
     SubsuperscriptBox["c", "i", "\[Dagger]"], 
     SubscriptBox["U", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}], TraditionalForm]],
  ExpressionUUID->"011e032a-1afd-40d2-b9e8-36746e2ed106"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],ExpressionUUID->
  "0f5cec38-bc4b-45eb-b1f8-6b428e3c7e91"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "j"], TraditionalForm]],ExpressionUUID->
  "96ef36c6-884d-45a2-b595-e85049a6e179"],
 " are ",
 StyleBox["bare",
  FontSlant->"Italic"],
 " and ",
 StyleBox["dressed",
  FontSlant->"Italic"],
 " fermionic modes, respectively."
}], "Notes",
 CellID->358018443,ExpressionUUID->"082f836c-5e2f-41b2-8458-016c4b23b3b0"],

Cell[TextData[{
 "Suppose  that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", 
     SubscriptBox["j", "1"]], "=", 
    RowBox[{
     SubscriptBox["v", 
      SubscriptBox["j", "2"]], "=", 
     RowBox[{"\[Ellipsis]", "=", 
      RowBox[{
       SubscriptBox["v", 
        SubscriptBox["j", "N"]], "=", "1"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"c9f88d07-1488-46ac-b98d-9b42fe9f3a2d"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "j"], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "0e566f44-b9c6-4fee-80bc-6d58462550bf"],
 " for all other ",
 Cell[BoxData[
  FormBox["j", TraditionalForm]],ExpressionUUID->
  "6d33ffc1-1df9-4e37-8c43-0112196bf735"],
 ", then it is equivalent to\n\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{"k", "=", "1"}], "N"], 
     SubsuperscriptBox["d", 
      SubscriptBox["j", "k"], "\[Dagger]"], 
     TemplateBox[{"\[SpaceIndicator]"},
      "Ket"]}], "\[Congruent]", 
    RowBox[{
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{"k", "=", "1"}], "N"], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{
       SubscriptBox["i", "k"], "=", "1"}], "L"], 
     SubsuperscriptBox["c", 
      SubscriptBox["i", "k"], "\[Dagger]"], 
     TemplateBox[{"\[SpaceIndicator]"},
      "Ket"], 
     SubscriptBox["U", 
      RowBox[{
       SubscriptBox["i", "k"], 
       SubscriptBox["j", "k"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "b9cbf13c-50d8-4a5c-af56-4c10539e9b7a"],
 "."
}], "Notes",
