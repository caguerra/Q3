  FormBox[
   RowBox[{
    OverscriptBox["W", "^"], ":=", 
    RowBox[{"exp", "(", 
     RowBox[{
      SubscriptBox["\[CapitalSigma]", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SubsuperscriptBox[
       OverscriptBox["c", "^"], "i", "\[Dagger]"], 
      SubscriptBox["A", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
