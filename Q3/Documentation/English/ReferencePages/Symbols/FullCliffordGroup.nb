Cell[TextData[{
 "The full Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"dbbb2b6c-b5ee-40f0-9798-863d5956089d"],
 " is the normalizer of the full Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"e5d2a20d-2f24-4075-b5fb-a86046b144db"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], ":=", 
    RowBox[{"{", 
     RowBox[{"U", ":", 
      RowBox[{
       RowBox[{"U", " ", "P", " ", 
        SuperscriptBox["U", "\[Dagger]"]}], "\[Element]", 
       RowBox[{
        RowBox[{
         OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "  ", 
        "for", " ", "all", " ", "P"}], "\[Element]", 
       RowBox[{
        OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}]}]}], "}"}]}],
    TraditionalForm]],ExpressionUUID->
  "30d51382-f25d-4cc5-ae41-fd2f94e12e11"],
 "."
}], "Notes",
 CellID->1836205616,ExpressionUUID->"6ceb1b3c-229e-4d70-9090-ff0dc3d581b6"],

Cell["\<\
The full Clifford group is generated by the Hadamard gates and the quadrant \
phase gates on individual qubits and the CNOT (CZ) gates on pairs of qubits.\
\>", "Notes",
 CellID->1029202184,ExpressionUUID->"170d89a6-9ec3-446d-9dfa-49ee4384c350"],

Cell[TextData[{
 "The Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"6bb6c4e4-a7b4-4143-a529-5f056354a697"],
