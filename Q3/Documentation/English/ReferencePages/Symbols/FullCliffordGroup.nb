 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"i", " ", "m", " ", 
          RowBox[{"\[Pi]", "/", "4"}]}]], ":", "m"}], "=", "0"}], ",", "1", 
      ",", "2", ",", "\[Ellipsis]", ",", "7"}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"a72c4867-dd87-4103-b5ca-df9b564c67f3"],
