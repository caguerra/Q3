transformed as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     TemplateBox[{
       RowBox[{
         SubscriptBox["c", "1"], ",", 
         SubscriptBox["c", "2"], ",", "\[Ellipsis]", ",", 
         SubscriptBox["c", "m"]}]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         SubscriptBox["t", "1"], ",", 
         SubscriptBox["t", "2"], ",", "\[Ellipsis]", ",", 
         SubscriptBox["t", "n"]}]},
      "Ket"]}], "\[RightTeeArrow]", 
    RowBox[{
     TemplateBox[{
       RowBox[{
         SubscriptBox["c", "1"], ",", 
         SubscriptBox["c", "2"], ",", "\[Ellipsis]", ",", 
         SubscriptBox["c", "m"]}]},
      "Ket"], "\[CircleTimes]", 
     TemplateBox[{
       RowBox[{
         RowBox[{
           RowBox[{"(", 
             RowBox[{
               SubscriptBox["c", "1"], "\[Wedge]", 
               SubscriptBox["c", "1"], "\[Wedge]", "\[CenterEllipsis]", 
               "\[Wedge]", 
               SubscriptBox["c", "m"]}], ")"}], "\[CirclePlus]", 
           SubscriptBox["t", "1"]}], ",", 
         RowBox[{
           RowBox[{"(", 
             RowBox[{
               SubscriptBox["c", "1"], "\[Wedge]", 
               SubscriptBox["c", "1"], "\[Wedge]", "\[CenterEllipsis]", 
               "\[Wedge]", 
               SubscriptBox["c", "m"]}], ")"}], "\[CirclePlus]", 
           SubscriptBox["t", "2"]}], ",", "\[Ellipsis]", ",", 
         RowBox[{
           RowBox[{"(", 
             RowBox[{
               SubscriptBox["c", "1"], "\[Wedge]", 
               SubscriptBox["c", "1"], "\[Wedge]", "\[CenterEllipsis]", 
               "\[Wedge]", 
               SubscriptBox["c", "m"]}], ")"}], "\[CirclePlus]", 
           SubscriptBox["t", "n"]}]}]},
      "Ket"]}]}], TraditionalForm]],ExpressionUUID->
  "129806ba-3405-4beb-a831-255b1f35455e"],
 " "
}], "Notes",
