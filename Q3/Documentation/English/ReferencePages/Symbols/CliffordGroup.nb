Cell[TextData[{
 "The full Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"64a7d49b-3fa1-49f1-b6df-8bd3458953f0"],
 " is the normalizer of the full Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"7f2ca2a8-f343-49ca-a30c-3f3344ab6b7c"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], ":=", 
    RowBox[{"{", 
     RowBox[{"U", ":", 
      RowBox[{
       RowBox[{"U", " ", "P", " ", 
        SuperscriptBox["U", "\[Dagger]"]}], "\[Element]", 
       RowBox[{
        RowBox[{
         OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "  ", 
        "for", " ", "all", " ", "P"}], "\[Element]", 
       RowBox[{
        OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}]}]}], "}"}]}],
    TraditionalForm]],ExpressionUUID->
  "88a1eb65-0c2c-4dcf-ac6c-14a3b3b9a145"],
 "."
}], "Notes",
 CellID->1836205616,ExpressionUUID->"b4d9e408-28d8-4ddd-8603-29eae7d72b97"],

Cell["\<\
The full Clifford group is generated by the Hadamard gates and the quadrant \
phase gates on individual qubits and the CNOT (CZ) gates on pairs of qubits.\
\>", "Notes",
 CellID->1029202184,ExpressionUUID->"283b47d7-9753-4259-a755-27ceec6dceba"],

Cell[TextData[{
 "The Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"c4b81999-b819-4ca9-8b63-10edda522462"],
 " is a quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], "/", 
     RowBox[{"\[ScriptCapitalZ]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"67d05683-9265-419e-bf5c-a6c6ee6f023d"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"7c42d9ed-de8a-4721-af9d-9a2f50697421"],
 " is the full Clifford group and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"i", " ", "m", " ", 
          RowBox[{"\[Pi]", "/", "4"}]}]], ":", "m"}], "=", "0"}], ",", "1", 
      ",", "2", ",", "\[Ellipsis]", ",", "7"}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"eca97898-4e44-4a4f-8dd2-f653f9823403"],
 "."
}], "Notes",
 CellID->1413575425,ExpressionUUID->"7c7aaa64-6264-4d8c-97db-6fac17b70114"],

Cell[TextData[{
 "The Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"1742de85-0250-4487-bad6-df3cb3208d90"],
 " is equivalent to the ",
 ButtonBox["binary symplectic group",
  BaseStyle->"Link",
  ButtonData->"paclet:Q3/ref/BinarySymplecticGroup"],
 " up to the Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"5a79e171-dd66-4f2d-b098-cf29f29d4c0e"],
 "; ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], "/", 
     RowBox[{"\[ScriptCapitalP]", "(", "n", ")"}]}], "\[TildeEqual]", 
    RowBox[{"Sp", "(", 
     RowBox[{
      RowBox[{"2", "n"}], ",", 
      SubscriptBox["\[DoubleStruckCapitalZ]", "2"]}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"5baf9c58-df6a-40bb-83f1-3dbf97c3efdc"],
 "."
}], "Notes",
 CellID->1023764637,ExpressionUUID->"46e221a3-ff13-4dbb-8792-d7423645ffe8"],

Cell[TextData[{
 "The order of the ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "db533c2c-3054-4bf3-a756-0dc41a20a4d0"],
 "-qubit Clifford group is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["2", 
     RowBox[{
      SuperscriptBox["n", "2"], "+", 
      RowBox[{"2", "n"}]}]], 
    RowBox[{
     UnderoverscriptBox["\[CapitalPi]", 
      RowBox[{"k", "=", "1"}], "n"], "(", 
     RowBox[{
      SuperscriptBox["4", "k"], "-", "1"}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"e09bdac5-bd0a-4da1-9dd5-5c4cf28cd747"],
