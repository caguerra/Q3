Cell[TextData[{
 "The full Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"64a7d49b-3fa1-49f1-b6df-8bd3458953f0"],
 " is the normalizer of the full Pauli group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], 
   TraditionalForm]],ExpressionUUID->"7f2ca2a8-f343-49ca-a30c-3f3344ab6b7c"],
 ", that is, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], ":=", 
    RowBox[{"{", 
     RowBox[{"U", ":", 
      RowBox[{
       RowBox[{"U", " ", "P", " ", 
        SuperscriptBox["U", "\[Dagger]"]}], "\[Element]", 
       RowBox[{
        RowBox[{
         OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}], "  ", 
        "for", " ", "all", " ", "P"}], "\[Element]", 
       RowBox[{
        OverscriptBox["\[ScriptCapitalP]", "~"], "(", "n", ")"}]}]}], "}"}]}],
    TraditionalForm]],ExpressionUUID->
  "88a1eb65-0c2c-4dcf-ac6c-14a3b3b9a145"],
 "."
}], "Notes",
 CellID->1836205616,ExpressionUUID->"b4d9e408-28d8-4ddd-8603-29eae7d72b97"],

Cell["\<\
The full Clifford group is generated by the Hadamard gates and the quadrant \
phase gates on individual qubits and the CNOT (CZ) gates on pairs of qubits.\
\>", "Notes",
 CellID->1029202184,ExpressionUUID->"283b47d7-9753-4259-a755-27ceec6dceba"],

Cell[TextData[{
 "The Clifford group ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"d5c82b13-db7c-44dd-aa49-90e67b615c40"],
 " is a quotient group ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalC]", "(", "n", ")"}], ":=", 
    RowBox[{
     RowBox[{
      OverscriptBox["\[ScriptCapitalC]", "~"], "(", "n", ")"}], "/", 
     RowBox[{"\[ScriptCapitalZ]", "(", "n", ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"95047470-0e82-4393-a28c-af19ae35e9bb"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["C", "~"], "(", "n", ")"}], TraditionalForm]],
  ExpressionUUID->"6084d9f1-89fd-445e-b2e6-13e2c10801aa"],
 " is the full Clifford group and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalZ]", ":=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"i", " ", "m", " ", 
          RowBox[{"\[Pi]", "/", "4"}]}]], ":", "m"}], "=", "0"}], ",", "1", 
      ",", "2", ",", "\[Ellipsis]", ",", "7"}], "}"}]}], TraditionalForm]],
  ExpressionUUID->"68d2c654-057c-4a14-aa0c-70bd51f55f80"],
 ". That is, ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[{
     Cell[
      TextData["CliffordGroup"]], "paclet:Q3/ref/CliffordGroup", 
     "Q3 Package Symbol"},
    "PackageLink",
    BaseStyle->"InlineFormula"], "[", 
   StyleBox["n", "TI"], "]"}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "b586ce9b-5025-4230-8f96-272c6db0b015"],
 " is a quotient group ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    TemplateBox[{
      Cell[
       TextData["FullCliffordGroup"]], "paclet:Q3/ref/FullCliffordGroup", 
      "Q3 Package Symbol"},
     "PackageLink",
     BaseStyle->"InlineFormula"], "[", 
    StyleBox["n", "TI"], "]"}], "/", 
   FormBox[
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"\[ImaginaryI]", "\[VeryThinSpace]", "m", "\[VeryThinSpace]", 
          RowBox[{
          "\[Pi]", "\[VeryThinSpace]", "/", "\[VeryThinSpace]", "4"}]}]], ":",
         "m"}], "=", "0"}], ",", "1", ",", "2", ",", "\[Ellipsis]", ",", 
      "7"}], "}"}],
    TraditionalForm]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "16458db3-f4e6-4613-8ad7-9a5645d5fef5"],
