Cell[TextData[{
 "Note that under a supermap, density matrices transform as\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Rho]", 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"]}]], "\[LongEqual]", 
         UnderscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}], LimitsPositioning -> True], 
         SubscriptBox[
          StyleBox["M", "TI"], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["j", "TI"], ";", 
            StyleBox["k", "TI"], 
            StyleBox["l", "TI"]}]], 
         SubscriptBox["\[Rho]", 
          RowBox[{
            StyleBox["k", "TI"], 
